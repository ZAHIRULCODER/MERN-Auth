{
	"version": 2,
	"builds": [
		{
			"src": "src/server.js",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/api/(.*)",
			"headers": [
				{
					"key": "Access-Control-Allow-Origin",
					"value": "*"
				},
				{
					"key": "Access-Control-Allow-Headers",
					"value": "Origin, X-Requested-With, Content-Type, Accept"
				},
				{
					"key": "Access-Control-Allow-Methods",
					"value": "GET, POST, PUT, DELETE, OPTIONS"
				}
			]
			
		},
		{
			"src": "/(.*)",
			"dest": "src/server.js"
		}
	]
}
